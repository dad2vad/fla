{
  "name": "zl-fetch",
  "version": "3.2.3",
  "description": "A library that makes the Fetch API a breeze",
  "main": "dist/index-cjs.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "prepublish": "npm run test && npm run build",
    "release": "np --no-publish && npm publish",
    "u": "npm i -g npm-check-updates && ncu -u && npm update && npm i",
    "su": "npm run build && npx surge publish ./dist --domain https://1ga.surge.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.9",
    "btoa": "^1.2.1",
    "cross-fetch": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "np": "^7.4.0",
    "portastic": "^1.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3",
    "supertest": "^6.1.3"
  }
}
